        "ANOVA (with Seaborn Dataset)"
      ],
      "metadata": {
        "id": "Pl2s5TrRNnZp"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "tips = sns.load_dataset(\"tips\")\n",
        "grouped = tips.groupby(\"day\")[\"total_bill\"].apply(list)\n",
        "f_stat, p_value = f_oneway(*grouped)\n",
        "print(\"ANOVA test p-value:\", p_value)\n"
      ],
