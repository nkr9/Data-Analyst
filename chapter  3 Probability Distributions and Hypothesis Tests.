        "Confidence Interval"
      ],
      "metadata": {
        "id": "1JS9J4VlMqIG"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "from scipy import stats\n",
        "\n",
        "data = np.random.normal(loc=50, scale=5, size=30)\n",
        "mean = np.mean(data)\n",
        "sem = stats.sem(data)\n",
        "ci = stats.t.interval(0.95, df=len(data)-1, loc=mean, scale=sem)\n",
        "\n",
        "print(\"95% Confidence Interval:\", ci)\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "zgKYpITcMsto",
        "outputId": "e5c6b0e7-406a-4972-d37d-46058f44ac2f"
      },
      "execution_count": 9,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "95% Confidence Interval: (np.float64(46.683933519848885), np.float64(49.997927306321294))\n"
          ]
        }
      ]
    },
