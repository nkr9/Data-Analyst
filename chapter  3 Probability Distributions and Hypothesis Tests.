      "metadata": {
        "id": "SU6s2vVeNFw6"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "sample_means = [np.mean(np.random.choice(data, size=30)) for _ in range(1000)]\n",
        "plt.hist(sample_means, bins=30, density=True)\n",
        "plt.title(\"Sampling Distribution of the Mean\")\n",
        "plt.xlabel(\"Sample Mean\")\n",
        "plt.ylabel(\"Frequency\")\n",
        "plt.grid(True)\n",
        "plt.show()\n"
      ],
